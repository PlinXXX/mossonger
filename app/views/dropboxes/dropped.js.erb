toastr['success']('<%= @dropable_type.capitalize %> #<%= @dropable_id %> archived');
$('.modal').fadeOut('fast', function() {
	$(this).remove();
});
$('.modal-backdrop.show').fadeOut('fast', function() {
	$(this).remove();
});
setTimeout(function(){
	$("#<%= @dropable_type %>-<%= @dropable_id %>").replaceWith(`<%= render @dropable %>`);
}, 100);